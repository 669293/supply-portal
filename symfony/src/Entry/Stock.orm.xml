<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="annotation\StockFiles" table="stock_files">
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="stock_files_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="provider" type="string" column="provider" length="12" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="date" type="date" column="date" nullable="false"/>
    <field name="datetime" type="datetime" column="datetime" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="invoice" type="string" column="invoice" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="comment" type="string" column="comment" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="note" type="string" column="invoice" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tax" type="decimal" column="tax" precision="10" scale="0" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="type" type="integer" column="type" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="params" type="integer" column="params" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="&quot;user&quot;" target-entity="Users" fetch="LAZY">
      <join-columns>
        <join-column name="&quot;user&quot;" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
