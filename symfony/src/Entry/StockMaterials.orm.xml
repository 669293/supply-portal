<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="annotation\StockMaterials" table="stock_materials">
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="stock_materials_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="title" type="string" column="title" length="1000" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="price" type="decimal" column="price" precision="10" scale="0" nullable="false">
      <options>
        <option name="default">0.0</option>
      </options>
    </field>
    <field name="count" type="decimal" column="count" precision="10" scale="0" nullable="false">
      <options>
        <option name="default">0.0</option>
      </options>
    </field>
    <field name="sum" type="decimal" column="sum" precision="10" scale="0" nullable="false">
      <options>
        <option name="default">0.0</option>
      </options>
    </field>
    <field name="tax" type="decimal" column="tax" precision="10" scale="0" nullable="false">
      <options>
        <option name="default">0.0</option>
      </options>
    </field>
    <field name="total" type="decimal" column="total" precision="10" scale="0" nullable="false">
      <options>
        <option name="default">0.0</option>
      </options>
    </field>
    <many-to-one field="unit" target-entity="Units" fetch="LAZY">
      <join-columns>
        <join-column name="unit" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="stock" target-entity="Stock" fetch="LAZY">
      <join-columns>
        <join-column name="stock" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
